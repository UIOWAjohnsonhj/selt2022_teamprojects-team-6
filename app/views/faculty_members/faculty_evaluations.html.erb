<h1>Faculty Evaluations</h1>
<h2>Hi <%= @display_name %></h2>
<h3>Student: <%= @student_name %></h3>
<table class="rwd-table">
  <tr>
    <th>Professor Name</th>
    <th>Date of Evaluation</th>
    <th>Score</th>
    <th>Comment</th>
  </tr>
  <tr>
    <%= @evaluation.each do |eval| %>
      <td><%= eval.professor %></td>
      <td><%= eval.evaluation_date %></td>
      <td><%= eval.score %></td>
      <td><%= eval.comment %></td>
    <%= end %>
  </tr>
</table>
