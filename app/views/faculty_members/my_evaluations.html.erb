<h1>My Evaluations</h1>
<h2>Hi <%= @display_name %></h2>
<form method="post" action="<%= main_index_path %>">
</form>
<table class="evaluations_table">
  <tr>
    <th>Student Name</th>
    <th>Email</th>
    <th>Applied Term</th>
    <th>Date of Evaluation</th>
    <th>Score</th>
    <th>Comment</th>
    <th>Status</th>
  </tr>

  <tr>
    <% unless @student_eval_dict.nil? %>
      <%  @student_eval_dict.each do |student, eval|  %>
          <td><%= student.first_name  %></td>
          <td><%= student.last_name %></td>
          <td><%= eval.applied_term  %></td>
          <td><%= eval.created_at %></td>
          <td><%= eval.score  %></td>
          <td><%= eval.comment %></td>
          <td><%= eval.status  %></td>
      <%  end  %>
    <%  end %>
  </tr>
</table>
