%h1 Log In

=form_tag main_view_profile_path, :method=>:post do

  =label :user, :email_address
  =text_field :user, :email
  =label :user, :password
  = password_field :user, :password

  %br/
  %br/

  = submit_tag "Login", :id => "login_button"

  %br/
  %br/
= link_to 'Forgot Password', main_index_path
