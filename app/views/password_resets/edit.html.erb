

<h2>Reset your password!</h2>
<form method="post" action="<%= password_reset_edit_path %>">
  <br>
  <label >New Password</label>
  <input id="password" placeholder="password" type="password_field">
  </br>
  <br>
  <label >Confirm New Password</label>
  <input id="password" placeholder="password_confirmation" type="password_field">
  </br>

  <br>
  <input id="submit" type="submit" type="reset_password">
  </br>
</form>
